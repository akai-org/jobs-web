{
  "extends": ["airbnb-base", "prettier", "plugin:react/recommended"],
  "rules": {
    "react/display-name": [{ "ignoreTranspilerName": true }]
  },
  "globals": {
    "navigator": true,
    "fetch": true,
    "window": true,
    "document": true,
    "it": true
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  }
}
